def bubble(n,arr):
  for i in range(n):
    for j in range(0,n-i-1):
      if arr[j]>arr[j+1]:
        arr[j],arr[j+1]=arr[j+1],arr[j]

n=int(input("Enter the total number of elements: "))
print("Enter elements: ")
arr={}
for i in range(0,n):
  arr[i]=int(input())
bubble(n,arr)
print("Sorted elements: ")
for i in range(n):
  print(arr[i])
